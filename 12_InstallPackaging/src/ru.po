#: guess_number.c:88
msgid "-h"
msgstr "-п"

#: guess_number.c:88
msgid "--help"
msgstr "--помощь"

#: guess_number.c:89
#, c-format
msgid "A rather stupid C program that guesses the number\n"
msgstr "Довольно глупая программа на Си, которая угадывает число\n"

#: guess_number.c:90
#, c-format
msgid "Args usage:\n"
msgstr "Использование аргументов:\n"

#: guess_number.c:91
#, c-format
msgid ""
"-h, --help\n"
"\tPrints the usage for the executable and exits.\n"
msgstr ""
"-п, --помощь\n"
"\tВыводит данные об использовании исполняемого файла и завершает работу.\n"

#: guess_number.c:92
#, c-format
msgid ""
"-r\n"
"\tEnables roman numbers.\n"
msgstr ""
"-р\n"
"\tВключает римские цифры.\n"

#: guess_number.c:94
msgid "-r"
msgstr "-р"

#: guess_number.c:95
#, c-format
msgid "Roman numerals are used\n"
msgstr "Используются римские цифры\n"

#: guess_number.c:98
#, c-format
msgid "Unknown command line argument\n"
msgstr "Неизвестный аргумент командной строки\n"

#: guess_number.c:105
#, c-format
msgid "Please guess the number from %s "
msgstr "Пожалуйста, угадайте число от %s "

#: guess_number.c:106
#, c-format
msgid "to %s\n"
msgstr "до %s\n"

#: guess_number.c:111
#, c-format
msgid "Is your number greater than %s?\n"
msgstr "Ваш номер больше, чем %s?\n"

#: guess_number.c:118
msgid "yes"
msgstr "да"

#: guess_number.c:118
msgid "y"
msgstr "д"

#: guess_number.c:120
msgid "no"
msgstr "нет"

#: guess_number.c:120
msgid "n"
msgstr "н"

#: guess_number.c:123
#, c-format
msgid "Just answer the question yes or no, please\n"
msgstr "Пожалуйста, отвечайте на вопрос да или нет\n"

#: guess_number.c:129
#, c-format
msgid "Your number is %s\n"
msgstr "Ваше число %s\n"
